
class Job:
    _counter = 0 
    def __init__(self, burstTime, arrivalTime):
        self.burstTime = burstTime
        self.arrivalTime = arrivalTime
        Job._counter +=1
        self.id = Job._counter

p1 = (3,0)
p2 = (2,1)
p3 = (7,2)

jobl = [Job]
    

def WaitingTime(orderlist):
    elapsed = 0
    for job in orderlist:
        elapsed += job.burstTime
        print(elapsed)

WaitingTime(orderlist)


